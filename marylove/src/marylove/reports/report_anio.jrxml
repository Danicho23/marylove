	<queryString language="SQL">
		<![CDATA[SELECT v.victima_codigo as "N",
p.persona_nombre ||'  '||p.persona_apellido as "Nombre",
i.ingreso_fecha as "Fecha de ingreso",
e.egreso_fecha as "Fecha de egreso",
(select count(*) from hijos h where h.victima_codigo =v.victima_codigo  )as NNA ,
(SELECT CASE WHEN (select victima_embarazo from victima where victima_embarazo = true) IS NOT
NULL THEN  '1'   ELSE  '' END) as "Embarazo",
(SELECT CASE WHEN (select llamada_lineaapoyo from registro_referencia rf where  v.victima_codigo = rf.victima_codigo  and rf.llamada_lineaapoyo = true) IS NOT
NULL THEN  '1'   ELSE  '' END) as "llamanda a la linea",
(select count(*)from registro_referencia rf where  v.victima_codigo = rf.victima_codigo ) as "Binvenida",

(select count(*) from plan_emergente pe where v.victima_codigo = pe.victima_codigo)as "plan emergente",
(select count(*) from plan_vida pv where v.victima_codigo = pv.victima_codigo)as "plan de vida",
(select count(*) from evaluacion_plan_vida epv  where v.victima_codigo = epv.victima_codigo)as "Evaluacion plan de vida",
(select count(*) from plan_autonomia pa where v.victima_codigo = pa.victima_codigo)as "plan de autonimia"
from persona p
join victima v
on v.persona_codigo = p.persona_codigo
join ingreso i
on i.victima_codigo = v.victima_codigo
left join egreso e
on e.victima_codigo = v.victima_codigo]]>
	</queryString>
